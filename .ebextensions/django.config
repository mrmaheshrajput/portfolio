container_commands:
  01_collectstatic:
    command: "source /opt/python/run/venv/bin/activate && python portfolio/manage.py collectstatic --noinput"

option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: portfolio.wsgi:application
  aws:elasticbeanstalk:environment:proxy:staticfiles:
    "/static/": "www/static/"
